.index_banner swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_banner swiper .pic {
  width: 100%;
}
.index_nav {
  display: flex;
}
.index_nav navigator {
  padding: 20px;
  flex: 1;
}
.index_nav navigator image {
  width: 100%;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
}
.index_floor .floor_group .floor_main {
  overflow: hidden;
}
.index_floor .floor_group .floor_main navigator {
  float: left;
  width: 33.33%;
}
.index_floor .floor_group .floor_main navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #ffffff;
}
.index_floor .floor_group .floor_main navigator:nth-child(2),
.index_floor .floor_group .floor_main navigator:nth-child(3) {
  border-bottom: 10rpx solid #ffffff;
}
.index_floor .floor_group .floor_main navigator image {
  width: 100%;
  height: 100%;
}
