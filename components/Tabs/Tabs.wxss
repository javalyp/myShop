.tabs .tab_title {
  background: #ffffff;
  display: flex;
}
.tabs .tab_title .title_item {
  flex: 1;
  justify-content: center;
  align-items: center;
  text-align: center;
  padding: 15rpx 0;
  border-bottom: 1px solid #ccc;
}
.tabs .tab_title .active {
  color: var(--themeColor);
  border-bottom: 2rpx solid currentColor;
}
